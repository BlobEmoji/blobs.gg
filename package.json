    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.6",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "prop-types": "^15.8.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intersection-observer": "^8.33.1",
    "react-router-dom": "^6.2.1"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.0.1",
