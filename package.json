    "@mui/material": "^5.0.2",
    "@mui/styles": "^5.0.1",
    "clsx": "^1.1.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intersection-observer": "^8.32.1",
    "react-router-dom": "^5.3.0"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.0.0",
    "@parcel/packager-xml": "^2.0.0",
